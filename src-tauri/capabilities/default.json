{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Allow launching tachidesk-server",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-open",
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "tachidesk-server",
          "cmd": "tachidesk-server"
        }
      ]
    }
  ]
}